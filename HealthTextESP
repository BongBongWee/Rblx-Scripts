local Players,RunService,LogService,LocalPlayer,active=game:GetService("Players"),game:GetService("RunService"),game:GetService("LogService"),game:GetService("Players").LocalPlayer,true;LogService.MessageOut:Connect(function(m)if m:lower():find("stoppinghpespv55")then for _,p in pairs(Players:GetPlayers())do if p.Character then local r=p.Character:FindFirstChild("HumanoidRootPart")if r then local g=r:FindFirstChild("HealthLabel")if g then g:Destroy()end end end end active=false end end);RunService.RenderStepped:Connect(function()if not active then return end for _,p in pairs(Players:GetPlayers())do if p~=LocalPlayer then pcall(function()local c=p.Character if not c then return end local r=c:FindFirstChild("HumanoidRootPart")local h=c:FindFirstChild("Humanoid")if not r or not h then return end local g=r:FindFirstChild("HealthLabel")if p.Team==LocalPlayer.Team then if g then g:Destroy()end return end if not g then local b=Instance.new("BillboardGui")b.Name="HealthLabel"b.Adornee=r b.Size=UDim2.new(0,100,0,12)b.StudsOffset=Vector3.new(0,-3.5,0)b.AlwaysOnTop=true local l=Instance.new("TextLabel")l.Name="TextLabel"l.Parent=b l.Size=UDim2.new(1,0,1,0)l.BackgroundTransparency=1 l.TextStrokeTransparency=0 l.Font=Enum.Font.SourceSansBold l.TextScaled=true b.Parent=r g=b end local l=g:FindFirstChild("TextLabel")l.Text="HP: "..math.floor(h.Health).." / 100"l.TextColor3=h.Health>75 and Color3.fromRGB(0,255,0)or h.Health>55 and Color3.fromRGB(255,255,0)or h.Health>20 and Color3.fromRGB(255,165,0)or Color3.fromRGB(255,0,0)end)end end end)
